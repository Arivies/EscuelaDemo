{% extends '::front.html.twig' %}

{% block body %}
    <h1>Carreras</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ entity.nombre }}</td>
            </tr>
        </tbody>
    </table>

    <ul class="record_actions">
        <li>
            <a href="{{ path('carreras') }}">
                Back to the list
            </a>
        </li>
    </ul>
{% endblock %}
